URxvt*title: urxvt
URxvt*termName: rxvt-256color
URxvt*secondaryscroll: true
URxvt*matcher.button: 1
URxvt*cursorBlink: true
URxvt*cursorUnderline: true
URxvt*mapAlert: true
URxvt*visualBell: true
URxvt*urgentOnBell: true
!URxvt*inheritPixmap: true
!URxvt*tintColor: #000000
!URxvt*shading: 88
 
URxvt*transparent: true 
URxvt*shading: 20
URxvt*saveLines: 12000
URxvt.font: xft:mono:medium:size=10
URxvt.boldFont: xft:Droid Sans Mono for Powerline:size=10
URxvt*scrollBar: false
URxvt*geometry: 90x21
URxvt.iconFile: /home/eregus/.icons/Faenza/apps/scalable/utilities-terminal.svg

URxvt.iso14755: False
 
URxvt.perl-lib: /usr/lib/urxvt/perl
URxvt.perl-ext-common: default,matcher,clipboard,selection-autotransform
URxvt.keysym.C-C: perl:clipboard:copy
URxvt.keysym.C-V: perl:clipboard:paste
URxvt.keysym.C-Z: perl:clipboard:paste_escaped
URxvt.clipboard.copycmd: xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
URxvt.url-launcher: /usr/bin/firefox-nightly
URxvt.matcher.button: 1
*depth: 32 
